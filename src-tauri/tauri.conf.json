{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "TDashcam Studio",
  "version": "1.0.1",
  "identifier": "com.tdashcam.studio",
  "build": {
    "frontendDist": "../src",
    "devUrl": "http://localhost:8188"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "TDashcam Studio",
        "width": 1400,
        "height": 900,
        "minWidth": 1024,
        "minHeight": 768,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "dragDropEnabled": true
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "binaries/ffmpeg"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {}
}
